
@{
    ViewBag.Title = "MyProfile";
}

<div class="container">
    <h3>Profile Details</h3>
    <div class="row">
        <div class="col-md-6">
            <table class="table table-striped table-hover">
                <tr>
                    <th>
                        Mobile
                    </th>
                    <td>
                        @ViewBag.ProfileDetail.Rows[0]["Mobile"]
                    </td>
                </tr>
                <tr>
                    <th>
                        EmailID
                    </th>
                    <td>
                        @ViewBag.ProfileDetail.Rows[0]["EmailID"]
                    </td>
                </tr>
                <tr>
                    <th>
                        Created Date
                    </th>
                    <td>
                        @ViewBag.ProfileDetail.Rows[0]["CreatedDate"]
                    </td>
                </tr>
                <tr>
                    <th>
                        Status
                    </th>
                    <td>
                        @ViewBag.ProfileDetail.Rows[0]["Status"]
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <h4>Used Equipment List</h4>
            <hr />
        </div>
    </div>
    <div class="row">
        <div class="col-md-12">
            <h4>Rental Post List</h4>
            <hr />
        </div>
    </div>
</div>

